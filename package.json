{
  "name": "operator-console",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-win-x64": "concurrently \"npm run start-server-win-x64\" \"npm run start\"",
    "dev-linux": "concurrently \"npm run start-server-linux\" \"npm run start\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "husky install",
    "prettify": "prettier . --write",
    "prettier:check": "prettier . --check",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "npm run lint --fix",
    "start-server-win-x64": "start ./server/RadioDataServer_win-x64.exe",
    "start-server-linux": "./server/RadioDataServer_linux-x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "rxjs": "^6.6.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint-config-prettier": "^7.1.0",
    "husky": "^5.0.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "proxy": "http://localhost:8080",
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  }
}
